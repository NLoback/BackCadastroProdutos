    "typescript": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.18.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.6"
  }
